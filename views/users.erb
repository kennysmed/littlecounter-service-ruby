<h2>Users</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Devices</th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |email, user| %>
      <tr>
        <td><%= user['name'] %></td>
        <td><%= user_devices(email).map { |d| d['name'] }.join(', ') %></td>
      </tr>
    <% end %>
  </tbody>
</table>
